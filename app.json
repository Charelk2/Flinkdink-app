
{
  "expo": {
    "name": "FlinkDink",
    "slug": "flinkdink",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saruman.flinkdink"
    },
    "android": {
      "package": "com.saruman.flinkdink",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "com.android.vending.BILLING"
      ]
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "jsEngine": "hermes"
          },
          "ios": {
            "jsEngine": "hermes"
          }
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "aeb77c78-6538-49dc-bc6d-7c16f4342933"
      },
      
      "revenueCatPublicKey": "goog_KbArquIXpiywIFZzosWvrYeVlFw",
      
      "revenueCatSecretKey": "REPLACE_WITH_YOUR_SECRET_KEY"
    }
  }
}
